10 INPUT "START VALUE:";R: INPUT "PASS:";A$
30 FOR I=1 TO LEN (A$)
40 A=ASC (MID$ (A$,I,1))
50 A=A-32+R
60 IF A>95 THEN A=A-96: GOTO 60
70 A=A+96/2
80 IF A>95 THEN A=A-96: GOTO 80
90 A=A+32
140 IF A=96 THEN A=44
150 IF A=127 THEN A=32
160 B$=B$+CHR$(A)
170 R=R+1: IF R>95 THEN R=0
190 NEXT I
200 PRINT B$
210 END
